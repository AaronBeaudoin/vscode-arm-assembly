            "extensions": [".s", ".S", ".i", ".asm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "arm-assembly",
            "scopeName": "source.armassem",
            "path": "./syntaxes/arm-assembly.tmLanguage.json"
        }]
    }
}
