{
    "name": "arm-assembly",
    "displayName": "arm-assembly",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "scripts": {
        "start": "js-yaml syntaxes/arm-assembly.tmLanguage.yaml > syntaxes/arm-assembly.tmLanguage.json"
    },
